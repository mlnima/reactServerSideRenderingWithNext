{
  "name": "reactServerSideRenderingWithNext",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/api-server",
    "apps/mail-server",
    "apps/file-server",
    "apps/custom-scripts",
    "apps/sitemaps-generator",
    "apps/socket-server",
    "apps/web-app",
    "apps/web-dashboard-app"
  ],
  "scripts": {
    "graph": "turbo run build test lint --graph=graph.jpg",
    "build": "turbo run build",
    "cleanup": "node cleanup.js",
    "start:api-server": "npm --prefix ./apps/api-server run start",
    "start:file-server": "npm --prefix ./apps/file-server run start",
    "start:mail-server": "npm --prefix ./apps/mail-server run start",
    "start:socket-server": "npm --prefix ./apps/socket-server run start",
    "start:web-app": "npm --prefix ./apps/web-app run start",
    "start:web-application": "npm --prefix ./apps/web-application run start",
    "start:sitemaps-generator": "npm --prefix ./apps/sitemaps-generator run start",
    "start": "concurrently \"npm run start:sitemaps-generator\" \"npm run start:api-server\" \"npm run start:file-server\"  \"npm run start:socket-server\" \"npm run start:web-app\" \"npm run start:mail-server\" ",
    "resetAdminPassword": "npm --prefix ./apps/custom-scripts run run-script:resetAdminPassword",
    "fixChanges": "npm --prefix ./apps/custom-scripts run run-script:fixChanges",
    "setup": "npm --prefix ./apps/custom-scripts run run-script:setup",
    "dev": "turbo run dev --parallel",
    "update:packages": "npm update --save/--save-dev -f",
    "outdate:packages": "npm outdated",
    "outdate:api-server-packages": "npm outdated -w api-server",
    "outdate:socket-server-packages": "npm outdated -w socket-server",
    "outdate:web-app-packages": "npm outdated -w web-app",
    "outdate:web-dashboard-app": "npm outdated -w web-dashboard-app",
    "outdate:sitemaps-generator-packages": "npm outdated -w sitemaps-generator",
    "outdate:custom-scripts-packages": "npm outdated -w custom-scripts",
    "killNodes": ".\\_devUtil\\windowsNodeProccessBulkKiller.bat"
  },
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "react-refresh": "^0.11.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.6",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/apicache": "^1.6.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.16",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.188",
    "@types/negotiator": "^0.6.1",
    "@types/node": "^18.16.14",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-fontawesome": "^1.6.5",
    "@types/react-redux": "^7.1.25",
    "@types/react-scroll": "^1.8.6",
    "@types/simple-peer": "^9.11.5",
    "@types/styled-components": "^5.1.26",
    "cookie-parser": "^1.4.6",
    "eslint": "8.45.0",
    "eslint-config-next": "^13.4.10",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-turbo": "^1.10.7",
    "eslint-plugin-react": "7.32.2",
    "express-xml-bodyparser": "^0.3.0",
    "jest": "^29.6.1",
    "jest-resolve": "^29.4.1",
    "jest-watch-typeahead": "^2.2.2 ",
    "ts-jest": "^29.1.1",
    "ts-lib": "^0.0.5",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig": "*",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "tslib": "^2.5.0",
    "turbo": "^1.10.7",
    "typescript": "^5.1.6"
  },
  "globalDependencies": [
    ".env"
  ]
}
