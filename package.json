{
  "name": "reactServerSideRenderingWithNext",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/api-server",
    "apps/custom-scripts",
    "apps/sitemaps-generator",
    "apps/socket-server",
    "apps/web-app",
    "apps/web-dashboard-app"
  ],
  "scripts": {
    "graph": "turbo run build test lint --graph=graph.jpg",
    "build": "turbo run build --parallel",
    "build:api-server": "turbo run build --filter=api-server",
    "build:socket-server": "turbo run build --filter=socket-server",
    "build:sitemaps-generator": "turbo run build --filter=sitemaps-generator",
    "build:web-app": "turbo run build --filter=web-app",
    "start:api-server": "npm --prefix ./apps/api-server run start",
    "start:socket-server": "npm --prefix ./apps/socket-server run start",
    "start:web-app": "npm --prefix ./apps/web-app run start",
    "start:sitemaps-generator": "npm --prefix ./apps/sitemaps-generator run start",
    "start": "concurrently \"npm run start:sitemaps-generator\" \"npm run start:api-server\"  \"npm run start:socket-server\" \"npm run start:web-app\" ",
    "resetAdminPassword": "npm --prefix ./apps/custom-scripts run run-script:resetAdminPassword",
    "setup": "npm --prefix ./apps/custom-scripts run run-script:setup",
    "dev": "turbo run dev",
    "update:packages": "npm update --save/--save-dev -f",
    "update:api-server-packages": "npm update --save/--save-dev -f --workspace=api-server",
    "update:socket-server-packages": "npm  update --save/--save-dev -f --workspace=socket-server",
    "update:web-app-packages": "npm  update --save/--save-dev -f --workspace=web-app",
    "update:sitemaps-generator-packages": "npm  update --save/--save-dev -f --workspace=sitemaps-generator",
    "update:custom-scripts-packages": "npm  update --save/--save-dev -f --workspace=custom-scripts",
    "killNodes": ".\\_devUtil\\windowsNodeProccessBulkKiller.bat"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.2",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/apicache": "^1.6.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.18",
    "@types/react-fontawesome": "^1.6.5",
    "@types/react-redux": "^7.1.25",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-scroll": "^1.8.6",
    "@types/styled-components": "^5.1.26",
    "@testing-library/user-event": "^14.4.3",
    "cookie-parser": "^1.4.6",
    "eslint": "8.31.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-turbo": "latest",
    "eslint-plugin-react": "7.31.11",
    "express-xml-bodyparser": "^0.3.0",
    "jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "tsc-alias": "^1.8.2",
    "tsconfig": "*",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.0",
    "turbo": "^1.7.0",
    "typescript": "^4.9.4",
    "ts-jest": "^29.0.3",
    "ts-lib": "^0.0.5"
  },
  "globalDependencies": [
    ".env"
  ]
}
