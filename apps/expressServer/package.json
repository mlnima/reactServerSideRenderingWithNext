{
  "name": "expressServer",
  "version": "1.0.0",
  "description": "Reacticum-server is a server side of Reacticum CMS project which is built using NextJs, Express, MongoDB...etc",
  "main": "index.js",
  "keywords": ["nextJs","turbo repo","next","ts"],
  "author": "02dev",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:server",
    "start": "concurrently \"NODE_ENV=production node ./.next/expressServer/index.js\" \"node ./.next/expressServer/socketServer/index.js\"",
    "dev": "concurrently \"cross-env nodemon --trace-warnings\"",
    "fixTimeStamsForPosts": "node tools/fixTimeStamsForPosts.js",
    "mergeDuplicateMetaData": "node tools/mergeDuplicateMetaData.js",
    "setup": "ts-node --transpile-only ./tools/setup.js",
    "ResetAdminPassword": "node ./tools/resetAdminPassword.js",
    "build:server": "tsc --project tsconfig.server.json",
    "update:packages": "npm update --save/--save-dev -f",
    "test": "jest --watch"
  },
  "dependencies":{
    "apicache": "^1.6.3",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.2",
    "dotenv-load": "^2.0.1",
    "express": "^4.18.1",
    "nodemon": "^2.0.18",
    "express-fileupload": "^1.4.0",
    "fs-extra": "^10.1.0",
    "image-downloader": "^4.3.0",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.14.181",
    "mailparser": "^3.5.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.3",
    "mongoose": "^6.6.0",
    "normalize-url": "^7.1.0",
    "path": "^0.12.7",
    "peerjs": "^1.4.7",
    "sharp": "^0.31.0",
    "simple-peer": "^9.11.1",
    "simple-youtube-api": "^5.2.1",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "ts-node": "^10.9.1",
    "uuid-apikey": "^1.5.3",
    "data-structures": "*",
    "custom-server-util": "*",
    "shelljs": "^0.8.5",
    "smtp-server": "^3.11.0",
    "jsonwebtoken": "^8.5.1",
    "tsconfig": "*",
    "custom-util": "*",
    "typescript-types": "*"
  }
}